{
  "builds": [
    {
      "src": "web-build/**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/web-build/index.html"
    },
    {
      "source": "/app.html",
      "destination": "/web-build/index.html"
    },
    {
      "source": "/app",
      "destination": "/web-build/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/web-build/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}