{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/app",
      "destination": "/app.html"
    },
    {
      "source": "/auth/callback",
      "destination": "/app.html"
    },
    {
      "source": "/(.*)",
      "destination": "/app.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}