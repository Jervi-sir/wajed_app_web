{
  "rewrites": [
    {
      "source": "/",
      "destination": "/home.html"
    },
    {
      "source": "/index.html",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/home.html" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}